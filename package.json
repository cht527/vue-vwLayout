  "description": "vw layout for vue project",
  "scripts": {
    "clean": "rimraf ./build/*",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --info false --progress  --hot --config ./config/webpack.dev.config.js",
    "analyze": "webpack --config ./config/webpack.analyze.config.js",
    "lint": "eslint --ext .js,.vue src",
    "fix": "eslint --ext .js,.vue src --fix"
